#include <iostream>
#include <vector> // needded so we can define vectors
/*
Basically writing code so it looks like: 
1 2 3 
4 5 6
7 8 9
*/
using namespace std;
int main(){
    vector<vector<int>> M{{1,2,3},{4,5,6},{7,8,9}}; // notice the nesting of vectors as dimension increases
    for(int i=0; i<M.size();i++){
        for(int j=0; j<M[i].size(); j++)
            cout << M[i][j]<< " ";
        cout << endl;
    }
    return 0;
}
